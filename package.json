    "typescript": "^4.2.3"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://sort-es.netlify.com",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
