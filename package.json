    "@typescript-eslint/parser": "^4.16.1",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.3",
    "codecov": "^3.8.1",
    "cosmiconfig": "^7.0.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.19.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
